<Window x:Class="TorKartingowy.WindowArchiwum"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TorKartingowy"
        mc:Ignorable="d"
        Title="WindowArchiwum" Height="300" Width="300" MinWidth="300" MinHeight="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="45*"/>
            <RowDefinition Height="224*"/>
        </Grid.RowDefinitions>
        <Button Content="Rekordy" Click="Button_Rekordy"/>
        <Button Content="Archiwum" Click="Button_Archiwum" Grid.Column="1"/>
        <Frame Name="FrameArchiwum" Content="FrameArchiwum" NavigationUIVisibility="Hidden" Grid.Row="1" Grid.ColumnSpan="2" Margin="0,0,0,0"/>
    </Grid>
</Window>
